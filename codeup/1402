package codeup;

import java.util.Scanner;

public class _1402_ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int []arr = new int[n];

		
	
		for(int i=0; i<n ;i++){
			
			arr[i] = sc.nextInt();
			
		}
		//System.out.println(arr[4]);
		
		for(int i=n-1; i>=0;i--){
			System.out.print(arr[i]);
	   }	
	}
} 
